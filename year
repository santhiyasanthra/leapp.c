#include <stdio.h>

void main()
{
int year;
printf("Enter a year: ");
scanf("%d",&year);
if(year%4 == 0)
{
if( year%100 == 0)
{
if ( year%400 == 0)
printf("%d- leap year", year);
else
printf("%d -leap year", year);
}
else
printf("%d-leap year", year );
}
else
printf("%d -not leap year", year);
}
getch();
